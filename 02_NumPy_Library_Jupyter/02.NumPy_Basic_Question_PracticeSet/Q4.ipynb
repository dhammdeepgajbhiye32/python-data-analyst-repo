   "source": [
    "import numpy as np\n",
    "\n",
    "arr = np.array([10, 60, 30, 80, 50, 90, 20])\n",
    "\n",
    "filtered_array = arr[arr > 50]\n",
    "\n",
    "print(f\"The Filtered values greater than 50 are: {filtered_array}\")"
   ]
